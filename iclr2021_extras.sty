\makeatletter
\DeclareRobustCommand{\rvdots}{%
  \vbox{
    \baselineskip4\p@\lineskiplimit\z@
    \kern-\p@
    \hbox{}\hbox{.}\hbox{.}\hbox{.}
  }}
\makeatother

